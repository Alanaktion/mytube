{
    "name": "alanaktion/mytube",
    "type": "project",
    "description": "A self-hosted YouTube mirror app",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "abraham/twitteroauth": "^2.0",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "1.0",
        "intervention/image": "^2.5",
        "laravel-lang/lang": "^8.1",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "meilisearch/meilisearch-php": "^0.18.0",
        "norkunas/youtube-dl-php": "^1.6",
        "predis/predis": "^1.1",
        "rebing/graphql-laravel": "^6.1",
        "romanzipp/laravel-twitch": "^4.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^0.7.12",
        "phpunit/phpunit": "^9.5.8",
        "rector/rector": "^0.11.40",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "YouTube"
        ]
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Google_Task_Composer::cleanup"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
