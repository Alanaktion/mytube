{
    "name": "alanaktion/mytube",
    "type": "project",
    "description": "A self-hostable video archive web app",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "abraham/twitteroauth": "^5.0",
        "doctrine/dbal": "^3.3",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel-lang/lang": "^12.0",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0",
        "laravel/telescope": "^4.13",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.0",
        "meilisearch/meilisearch-php": "^1.0",
        "norkunas/youtube-dl-php": "^2.0",
        "opcodesio/log-viewer": "^2.1",
        "predis/predis": "^2.0",
        "rebing/graphql-laravel": "^8.2",
        "romanzipp/laravel-twitch": "^4.5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.15",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Google_Task_Composer::cleanup",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "YouTube"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.13"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
