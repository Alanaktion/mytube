{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "abraham/twitteroauth": "^4.0",
        "doctrine/dbal": "^3.3",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel-lang/lang": "^10.0",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/scout": "^9.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "meilisearch/meilisearch-php": "^0.23.1",
        "norkunas/youtube-dl-php": "^2.0",
        "predis/predis": "^1.1",
        "rebing/graphql-laravel": "^8.2",
        "romanzipp/laravel-twitch": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "rector/rector": "^0.12.1",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Google_Task_Composer::cleanup",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "YouTube"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.20"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
